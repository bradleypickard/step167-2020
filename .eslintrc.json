{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "globals": {
    "firebase": true
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "no-multi-spaces": "off",
    "require-jsdoc": "off",
    "valid-jsdoc": "off"
  }
}
